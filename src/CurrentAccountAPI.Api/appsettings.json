  },
  "Swagger": {
    "SwaggerGenOptions": {
      "SwaggerGeneratorOptions": {
        "SwaggerDocs": {
          "v1": {
            "Title": "Current Account API",
            "Version": "v1"
          }
        }
      },
      "SchemaGeneratorOptions": {
      }
    },
    "SwaggerUIOptions": {
      "RoutePrefix": "swagger",
      "DocumentTitle": "CurrentAccountAPI",
      "ConfigObject": {
        "Urls": [
          {
            "Url": "v1/swagger.json",
            "Name": "V1 Docs"
          }
